---
layout: null
---
[
  {% for item in site.software %}
    {
      {% for kv in item %}
        {% unless kv[0] == 'next' or kv[0] == 'previous' %}
          {{ kv[0] | jsonify }}: {{ kv[1] | jsonify }}
          {% unless forloop.last %},{% endunless %}
        {% endunless %}
      {% endfor %}
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
